{
	"name": "RoomEase Dev Container",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"workspaceFolder": "/workspaces/RoomEase",
	"customizations": {
		"vscode": {
			"extensions": [
				"mongodb.mongodb-vscode",
				"dbaeumer.vscode-eslint",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"forwardPorts": [3000, 27017, 8080],
	"postCreateCommand": "npm install && cd frontend && npm install",
	"remoteUser": "node"
}
